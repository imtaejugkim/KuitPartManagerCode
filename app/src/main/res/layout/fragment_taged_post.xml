<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        tools:context=".profile.TagedPostFragment">

    <GridLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            android:orientation="horizontal"
            android:columnCount="3"
            android:rowCount="2">

        <ImageView
                android:id="@+id/iv_taggedPost1"
                android:layout_width="119dp"
                android:layout_height="119dp"
                android:padding="1dp"
                android:scaleType="centerCrop" />

        <ImageView
                android:id="@+id/iv_taggedPost2"
                android:layout_width="119dp"
                android:layout_height="119dp"
                android:padding="1dp"
                android:scaleType="centerCrop"/>
        <ImageView
                android:id="@+id/iv_taggedPost3"
                android:layout_width="119dp"
                android:layout_height="119dp"
                android:padding="1dp"
                android:scaleType="centerCrop"/>
        <ImageView
                android:id="@+id/iv_taggedPost4"
                android:layout_width="119dp"
                android:layout_height="119dp"
                android:padding="1dp"
                android:scaleType="centerCrop"/>
        <ImageView
                android:id="@+id/iv_taggedPost5"
                android:layout_width="119dp"
                android:layout_height="119dp"
                android:padding="1dp"
                android:scaleType="centerCrop"/>
        <ImageView
                android:id="@+id/iv_taggedPost6"
                android:layout_width="119dp"
                android:layout_height="119dp"
                android:padding="1dp"
                android:scaleType="centerCrop"/>


    </GridLayout>

</androidx.constraintlayout.widget.ConstraintLayout>